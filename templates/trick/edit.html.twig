{% extends "base.html.twig" %}

{% block title %}
	Modifier un trick
{% endblock %}

{% block body %}
	<h1>Modifier un trick</h1>

	{{ form_start(formView) }}

	<div>
		{{ form_row(formView.name) }}
	</div>

	<div>
		{{ form_row(formView.description) }}
	</div>

	<div>
		{{ form_row(formView.category) }}
	</div>

	<button type="submit">Modifier</button>

	{{ form_end(formView) }}


{% endblock %}
